programa {
  funcao inicio() {

    //Declaração de Variáveis.
    cadeia nome
    inteiro idade

    //Solicitar dados.
    escreva("Digite seu Nome: ")
    leia(nome)

    escreva("Digite sua Idade: ")
    leia(idade)

    //Resultado.

    se (idade>=18 e idade <=64) {
      escreva("\nVOCÊ É OBRIGADO A VOTAR")
    } senao 
      escreva("\nVocê não é obrigado a votar")
    }
    
  

    
    

  }
}
